<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="login" namespace="/login"
		extends="crm-default">
		<action name="index" class="loginAction" method="index">
		    <result name="success">main.jsp</result>
		    <result name="error">login.jsp</result>
		</action>
		<action name="toPropagandaPage" class="loginAction" method="toPropagandaPage">
		    <result name="success">propaganda.html</result>
		    <result name="error">login.jsp</result>
		</action>
		<action name="toDownChrome" class="loginAction" method="toDownChrome">
		    <result name="success">downChrome.html</result>
		</action>
		<action name="main" class="loginAction" method="main">
		    <result name="success">main.jsp</result>
		</action>
		<action name="top" class="loginAction" method="top">
		    <result name="success">top.jsp</result>
		</action>
		<action name="login" class="loginAction" method="login">
		    <result name="success" type="json"></result>
		    <result name="error" type="json"></result>
		</action>
		<action name="userLogout" class="loginAction" method="userLogout">
		    <result name="success" type="json"></result>
		    <result name="error" type="json"></result>
		</action>
		<action name="currentLoginUserInfo" class="loginAction" method="currentLoginUserInfo">
		    <result name="success" type="json"></result>
		    <result name="error" type="json"></result>
		</action>
	</package>
</struts>
