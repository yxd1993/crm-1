    <connection>dc</connection>
    <sql>select yd.ydbh as billNum, NVL&#x28;yd.hjje,0&#x29; as billingAmount,yd.lrsj as billingDate,yd.TYRQ as checkDate,yd.shrlxr as consignee,yd.tyrbh as dcAccount,&#xd;&#xa;yd.mddmc as destination,yd.tyrlxr as shipper,yd.qydmc as startingPoint,yd.cyrqz as transportType,&#x27;sysadmin&#x27; as CREATEUSER,sysdate as CREATEDATE,&#x27;Y&#x27; as ACTIVE,&#xd;&#xa;NVL&#x28;YD.FQYSJE, 0&#x29; &#x2b; NVL&#x28;YD.DQJSYSJE, 0&#x29;&#xd;&#xa;&#x2b; NVL&#x28;YD.SHRFKYSJE, 0&#x29; &#x2b; NVL&#x28;YD.SHRJSYS, 0&#x29;&#xd;&#xa;&#x2b; NVL&#x28;YD.DSFZFYS, 0&#x29; as incomeAmount,&#xd;&#xa;CASE WHEN NVL&#x28;YD.FQYSJE, 0&#x29; &#x2b; NVL&#x28;YD.DQJSYSJE, 0&#x29; &#x3e; 0 THEN YD.QYDMC&#xd;&#xa;WHEN NVL&#x28;YD.SHRFKYSJE, 0&#x29; &#x2b; NVL&#x28;YD.SHRJSYS, 0&#x29; &#x3e; 0 THEN GS.GSJC&#xd;&#xa;WHEN NVL&#x28;YD.DSFZFYS, 0&#x29; &#x3e; 0 THEN gs2.gsjc END as incomeDept,&#x27;Y&#x27; as isMessage&#xd;&#xa;from hydata.ld_yd yd&#xd;&#xa;left join hydata.ld_gs gs on yd.MDDZBH &#x3d; gs.gsbh&#xd;&#xa;left join hydata.ld_gs gs2 on yd.Ldyszgs &#x3d; gs2.gsbh&#xd;&#xa;where TO_CHAR&#x28;yd.record_date,&#x27;YYYY-MM-DD HH24&#x3a;MI&#x3a;SS&#x27;&#x29; &#x3e;&#x3d; &#x3f; and yd.tyrbh is not null and ydzt &#x21;&#x3d; 5</sql>
    <limit>0</limit>
    <lookup>&#x83b7;&#x53d6;&#x7cfb;&#x7edf;&#x4fe1;&#x606f;</lookup>
